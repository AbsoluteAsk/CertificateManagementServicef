  "Serilog": {
    "LogGroup": "/log/demo",
    "Region": "ap-south-1",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "AWSSeriLog",
        "Args": {
          "textFormatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "ServiceConfiguration": {
    "AWSSQS": {
      "QueueUrl": "http://localhost:4566/000000000000/certificateService_queue"
    }
  },
  "AllowedHosts": "*"
}
